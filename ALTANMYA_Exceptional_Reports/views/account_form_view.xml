<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_form_inherited" model="ir.ui.view">
        <field name="name">account.form.inherited</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@id='header_left_group'][1]/field[1]" position="after">
                <field name="test"/>
                <field name="sale_order_id"/>
                <field name="partner_bank_ids"/>
            </xpath>
        </field>
    </record>

    <record id="view_move_line_form_inh" model="ir.ui.view">
        <field name="name">account.move.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@id='invoice_tab'][1]/field[1]/tree[1]/field[10]" position="after">

                <field name="result" readonly="1"/>
            </xpath>
        </field>
    </record>

</odoo>